# File managed by Chef

<% @allows.each do |allow| -%>
allow <%= allow %>;
<% end -%>

<% @parameters.each do |parameter| -%>
<%= parameter %>;
<% end -%>

<% @options.each do |option| -%>
option <%= option %>;
<% end -%>

include "<%= @conf_dir %>/groups.d/group_list.conf";
include "<%= @conf_dir %>/subnets.d/subnet_list.conf";
include "<%= @conf_dir %>/hosts.d/host_list.conf";

